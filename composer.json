{
  "name": "jpmurray/xcancelbot",
  "type": "project",
  "description": "X Cancel bot is your link-cleanup hero for Discord.",
  "keywords": [
    "discord",
    "bot",
    "x",
    "twitter",
    "xcancel"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.2"
  },
  "require-dev": {
    "fakerphp/faker": "^1.23",
    "laravel/pint": "^1.15",
    "laracord/framework": "^2.3"
  },
  "bin": [
    "builds/xcancelbot"
  ],
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
